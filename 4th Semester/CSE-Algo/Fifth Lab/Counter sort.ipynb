   "source": [
    "array = [1, 3, 5, 4, 3, 1]\n",
    "newarr = array.copy()\n",
    "maximum = max(array)\n",
    "count = [0 for _ in range(maximum + 1)]\n",
    "\n",
    "for i in array:\n",
    "    count[i] += 1\n",
    "    \n",
    "for i in range(1, maximum + 1):\n",
    "    count[i] += count[i - 1]\n",
    "\n",
    "for i in array:\n",
    "    count[i] -= 1\n",
    "    newarr[count[i]] = i\n",
    "\n",
    "print(newarr)"
   ]
  }
